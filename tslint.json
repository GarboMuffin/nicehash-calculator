{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": false,
    "no-empty": false,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ],
    "no-trailing-whitespace": [true, "ignore-jsdoc"],
    "no-console": false,
    "member-ordering": [
      true, {
        "order": [
          "static-field",
          "static-method",
          "instance-field",
          "constructor",
          "instance-method"
        ]
      }
    ],
    "object-literal-sort-keys": false,
    "member-access": [true, "check-parameter-property"],
    "ordered-imports": false,
    "object-literal-key-quotes": false,
    "max-line-length": false
  },
  "rulesDirectory": []
}
